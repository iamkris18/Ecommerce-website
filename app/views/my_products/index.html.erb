<p id="notice"><%= notice %></p>
<%=link_to "Home",root_path%>
<%if user_signed_in?%>

<%=link_to "Log out",destroy_user_session_path%>


<%else%>
<%=link_to "Log in",new_user_session_path%>
<%=link_to "Sign up",new_user_registration_path%>

<%end%>
<h1>My Products</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Quantity</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @my_products.each do |my_product| %>
      <tr>
        <td><%= my_product.title %></td>
        <td><%= my_product.quantity %></td>
        <td><%= link_to 'Show', my_product %></td>
        <td><%= link_to 'Edit', edit_my_product_path(my_product) %></td>
        <td><%= link_to 'Destroy', my_product, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New My Product', new_my_product_path %>
